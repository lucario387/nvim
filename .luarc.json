{
  "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
  "format": {
    "enable": true
  },
  "semantic": {
    "annotation": true,
    "enable": true
  },
  "completion": {
    "enable": true,
    "callSnippet": "Disable"
  },
  "diagnostics": {
    "globals": [
      "vim"
    ],
    "ignoredFiles": "Disable",
    "libraryFiles": "Disable"
  },
  "runtime": {
    "version": "LuaJIT",
    "pathStrict": true,
    "path": [
      "lua/?.lua",
      "lua/?/init.lua"
    ]
  },
  "workspace": {
    "checkThirdParty": false,
    "library": [
      "$VIMRUNTIME/lua",
      "${3rd}/luv/library",
      "/home/lucario387/dev/ui/nvchad_types",
      "/home/lucario387/.local/share/nvim/lazy/lazy.nvim/lua",
      "/home/lucario387/.local/share/nvim/lazy/nvim-jdtls/lua",
      "/home/lucario387/.local/share/nvim/lazy/base46/lua",
      "/home/lucario387/.local/share/nvim/lazy/nvim-cmp/lua"
    ]
  },
  "hover": {
    "enable": true,
    "expandAlias": false,
    "enumsLimit": 5,
    "previewFields": 5
  },
  "hint": {
    "enable": true
  },
  "codeLens": {
    "enable": true
  }
}
