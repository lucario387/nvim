{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "workspace": {
    "checkThirdParty": false,
    "maxPreload": 2500,
    "preloadFileSize": 20000,
    "ignoreSubmodules": true,
    "library": [
      "/home/lucario387/.config/nvim",
      "/usr/local/share/nvim/runtime",
      "/home/lucario387/.local/share/nvim/site/pack/packer/start"
    ]
  },
  "runtime": {
    "version": "LuaJIT",
    "path": [
      "?.lua",
      "?/init.lua",
      "lua/?.lua",
      "lua/?/init.lua"
    ]
  },
  "diagnostics": {
    "workspaceRate": 100,
    "libraryFiles": "Disable",
    "globals": [
      "vim"
    ],
    "groupFileStatus": {
      "ambiguity": "Opened",
      "await": "Opened"
    },
    "neededFileStatus": {
      "codestyle-check": "Opened!"
    }
  },
  "format": {
    "defaultConfig": {
      "$schema": "https://github.com/CppCXY/EmmyLuaCodeStyle/blob/master/docs/format_config_EN.md",
      "align_call_args": "false",
      "align_function_define_params": "true",
      "continuation_indent_size": "2",
      "continuous_assign_statement_align_to_equal_sign": "true",
      "continuous_assign_table_field_align_to_equal_sign": "true",
      "if_condition_align_with_each_other": "true",
      "if_condition_no_continuation_indent": "true",
      "indent_size": "2",
      "indent_style": "space",
      "insert_final_newline": "true",
      "keep_one_space_between_namedef_and_attribute": "true",
      "keep_one_space_between_table_and_braket": "true",
      "label_no_indent": "true",
      "quote_style": "double"
    },
    "enable": true
  },
  "completion": {
    "callSnippet": "Both"
  },
  "hint": {
    "enable": true,
    "setType": true
  },
  "hover": {
    "enable": true,
    "expandAlias": true,
    "enumsLimit": 5,
    "previewFields": 100
  },
  "telemetry": {
    "enable": false
  },
  "semantic": {
    "enable": true
  }
}
